pipeline {
    agent any

    stages {
       stage ('BUILD') {
           steps {
                sh  'echo "test file create"'
           }
       } 
       
       stage ('TEST') {
           steps {
                sh 'echo "file to test"'
           }
       }
        
       stage ('DEPLOY') {
           steps {
                sh 'echo "file to deploy"'
           }
       }  
       stage ('DEPLOY1') {
           steps {
                sh 'echo "file to deploy second time"'
           }
       }     
    }
}
