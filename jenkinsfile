pipeline {
    agent any

    stages {
       stage ('BUILD') {
           steps {
                sh 'test file build'
           }
       } 
       
       stage ('TEST') {
           steps {
                sh 'file to test'
           }
       }
        
       stage ('DEPLOY') {
           steps {
                sh 'file to deploy'
           }
       }  
       stage ('DEPLOY1') {
           steps {
                sh 'file to deploy second time'
           }
       }     
    }
}
